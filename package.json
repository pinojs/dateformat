{
  "name": "@pinojs/dateformat",
  "version": "1.0.0-rc.1",
  "description": "fast formatter for dates",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build:types": "npm run delete:types && tsc --project tsconfig.types.json",
    "delete:types": "node -e \"import { rmSync } from 'fs'; try {rmSync('./index.d.ts'); } catch {} \"",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "node --test",
    "test:coverage": "c8 --reporter=html npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinojs/dateformat.git"
  },
  "keywords": [
    "date",
    "format",
    "string"
  ],
  "author": {
    "name": "Aras Abbasi",
    "email": "aras.abbasi@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinojs/dateformat/issues"
  },
  "homepage": "https://github.com/pinojs/dateformat#readme",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "c8": "^10.1.3",
    "eslint": "^9.32.0",
    "neostandard": "^0.12.2",
    "tinybench": "^4.0.1"
  }
}
